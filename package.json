{
  "name": "cash-trading-game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:all": "concurrently -n web,api -c blue,magenta \"npm run dev\" \"cd server && npm run dev\"",
    "db:clear": "cd server && npm run db:clear",
    "db:seed": "cd server && npm run db:seed",
    "db:reset": "npm run db:wipe && npm run db:seed",
    "db:wipe": "rm -f server/database/game.db"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^5.0.0",
    "@aptos-labs/wallet-adapter-react": "^7.1.0",
    "openai": "^5.23.0",
    "p5": "^2.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.3"
  }
}
